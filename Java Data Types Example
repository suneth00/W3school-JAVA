public class Main {
    public static void main(String[] args){

        int item = 50;
        float costPerItem = 9.99f;
        float totalCost = item * costPerItem;
        char currency = '$';


        System.out.println(item);
        System.out.println(costPerItem);
        System.out.println(totalCost);
        System.out.println(currency);




    }
}
